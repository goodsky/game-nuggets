          <Tooltip>Home Button</Tooltip>
          <IconImage>toolbar-icon-test1</IconImage>
          <OnSelect x:type="PopUpSubMenuAction" buttonGroupName="HomeMenu"/>
          <OnDeselect x:type="PopDownSubMenuAction" />
