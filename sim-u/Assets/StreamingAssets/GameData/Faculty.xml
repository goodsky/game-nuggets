<FacultyData xmlns:x="http://www.w3.org/2001/XMLSchema-instance">

  <DefaultHeadshot>DefaultIcon</DefaultHeadshot>
  <AvailableFacultyCount>5</AvailableFacultyCount>

  <!-- Untaught students are taught by "Street" which has the Default teaching score-->
  <TeachingScore min="50" max="100" default="50" />
  <ResearchScore min="50" max="100" default="50" />
  <FacultySlots min="2" max="4" default="2" />

  <MinimumBaseSalary>10000</MinimumBaseSalary>
  <AverageDollarsPerTeachingPoint>700</AverageDollarsPerTeachingPoint>
  <AverageDollarsPerResearchPoint>700</AverageDollarsPerResearchPoint>
  
  <TeachingScoreStdDev>7.5</TeachingScoreStdDev>
  <ResearchScoreStdDev>5</ResearchScoreStdDev>
  
  <!-- The default median of teacher distributions. Linearly scales to score max. -->
  <MinDefaultTeachingScore>70</MinDefaultTeachingScore>
  <MinDefaultResearchScore>50</MinDefaultResearchScore>

  <!-- The amount that research or teaching impacts hiring professors with the opposite skill [0, 1] -->
  <ResearchPrestigeImpactOnTeachingScore>0.1</ResearchPrestigeImpactOnTeachingScore>
  <TeachingPrestigeImpactOnResearchScore>0.1</TeachingPrestigeImpactOnResearchScore>

  <!-- Faculty that are above your current median cost a premium to hire -->
  <FacultyOverTheMeanPremiumMaxDollars>30000</FacultyOverTheMeanPremiumMaxDollars>
  <FacultyOverTheMeanPremiumExponentialFactor>1.2</FacultyOverTheMeanPremiumExponentialFactor>
  
  <!-- Factor of how much better "good" research scores are. -->
  <ResearchExponentialFactor>2.0</ResearchExponentialFactor>
  
